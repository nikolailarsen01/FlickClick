@{
    ViewData["Title"] = "Movies";
    ViewData["Stylesheet"] = "/css/cmsMovies.css";
}

<div class="movie-list">
    @foreach (var item in Model)
    {
        <div class="movie-list-item">
            <p>"@item.title"</p>
        </div>
    }
</div>