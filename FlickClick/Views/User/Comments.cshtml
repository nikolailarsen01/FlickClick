@model List<CommentModel>

@{
    Layout = null;
}
    <tr>
        <th>
            Movie
        </th>
        <th>
            Comment
        </th>
        <th>
            Date
        </th>
    </tr>
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>
                @Model[i].username
            </td>
            <td>
                @Model[i].comment
            </td>
            <td>
                @Model[i].date
            </td>
        </tr>
    }